{
    "manifest_version": 2,
    
    "name":         "spFilterWebContent",
    "description":  "watch and block webrequests of the browser",
    "author":       "terriskator",
    "version":      "4.2.35",
    
    "browser_action": {
	"default_icon": {
	    "16": "spFilterWebContent.logo.016.png",
	    "32": "spFilterWebContent.logo.032.png",
	    "64": "spFilterWebContent.logo.064.png",
	    "128": "spFilterWebContent.logo.128.png"
	},
	"default_title": "spFilterWebContent"
    },
    
    "permissions": [
	"downloads",
	"tabs",
	"webRequest",
	"webRequestBlocking",
	"storage",
	"<all_urls>"
    ],
    
    "background": {
	"scripts": ["spFilterWebContent.background.js"]
    },
    
  "options_page": "spFilterWebContent.options.html",

  "update_url":   "http://127.0.0.1/spFilterWebContent_update.xml",


    "icons": {
	"16": "spFilterWebContent.logo.016.png",
	"32": "spFilterWebContent.logo.032.png",
	"64": "spFilterWebContent.logo.064.png",
	"128": "spFilterWebContent.logo.128.png",
	"256": "spFilterWebContent.logo.256.png"
    }
}
